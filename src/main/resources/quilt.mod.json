{
	"schema_version": 1,
	"quilt_loader": {
		"group": "com.adryd",
		"id": "uselesschatreports",
		"version": "${version}",
		"metadata": {
			"name": "Useless Chat Reports",
			"description": "Proof of concept demonstrating that you can sign and send your messages with any timestamp, and servers/clients just accept it. This means if your messages get reported, the context will be completely scrambled",
			"contributors": {
				"adryd": ""
			},
			"contact": {
				"homepage": "https://adryd.com/",
				"issues": "https://github.com/adryd325/UselessChatReports/issues",
				"sources": "https://github.com/adryd325/UselessChatReports"
			},
			"icon": "assets/example_mod/icon.png"
		},
		"intermediate_mappings": "net.fabricmc:intermediary",
		"entrypoints": {
			"init": "com.adryd.uselesschatreports.UselessChatReportsEntry"
		},
		"depends": [
			{
				"id": "quilt_loader",
				"versions": ">=0.17.0-"
			},
			{
				"id": "minecraft",
				"versions": ">=1.19"
			}
		]
	},
	"mixin": "uselesschatreports.mixins.json"
}
